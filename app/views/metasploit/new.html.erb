<h1>Upload Metasploit File</h1>
<%= form_tag engagement_metasploit_index_path(@engagement), multipart: true do %>
	<div class="row">
		<div class="col-md-4 col-md-offset-4">
			<%= label_tag "Upload metasploit file (should be in xml format)" %>
			<%= file_field_tag 'metasploit_file' %>
			<%= submit_tag %>
		</div>
	</div>
	
<% end %>