<div class="noprint">
	<h2 class="cwe_h2">
		CWE-<%= @cwe.weakness_id %>: <%= @cwe.name %>
	</h2>
	<div class="header_div">
		<ul id="navPyra"></ul>
	</div>
</div>
<div id="CWEDefinition" class="Weakness">
	<div class="title">
		<div align="center">
			<span>
				<%= @cwe.name %>
			</span>
		</div>
	</div>
	<div class="status">
		<div>
			<div class="col-md-4 pLeft0" valign="top" nowrap="">
				<b>Weakness ID: </b><%= @cwe.weakness_id %>
				<span>(Weakness <%= @cwe.weakness_abstruction %>)</span>
			</div>
			<div class="col-md-offset-4 cweRight" valign="top" nowrap="">
				<b>Status: </b>
				<span><%= @cwe.weakness_status %></span>
			</div>
		</div>
	</div>
	<div class="Description">
		<div class="panel-group" id="desc_accordion" role="tablist" aria-multiselectable="true">
			<div class="panel panel-default">
				<div class="panel-heading" role="tab" id="headingOne">
					<div class="heading">
						<span id="script">
							<%= link_to "#collapseDesc", "data-toggle" => "collapse", "data-parent" => "#desc_accordion", "aria-expanded" => "true", "aria-controls" => "collapseDesc" do %>
								<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
							<% end %>
						</span>
						Description
					</div>
				</div>
				<div id="collapseDesc" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
					<div class="panel-body">
						<div id="oc_<%= @cwe.weakness_id %>_Description" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Description">
							<div class="detail">
								<p class="subheading">Description Summary</p>
								<div class="indent">
									<%= @cwe.desc_summary %>
								</div>
								<% unless @cwe.ext_desc.blank? %>
									<p class="subheading">Extended Summary</p>
									<div class="indent">
										<% @cwe.ext_desc.split('\n').each do |ext_desc| %>
											<p>
												<%= ext_desc %>
											</p>
										<% end %>
									</div>
								<% end %>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<% unless @cwe.terminology_note.blank? %>
		<div id="Terminology_Notes">
			<div class="panel-group" id="terminology_notes_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapseTerminologyNotes", "data-toggle" => "collapse", "data-parent" => "#terminology_notes_accordion", "aria-expanded" => "true", "aria-controls" => "collapseDesc" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Terminology Notes
						</div>
					</div>
					<div id="collapseTerminologyNotes" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Terminology_Notes" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Terminology_Notes">
								<div class="detail">
									<div id="Grouped">
										<div id="Detail">
											<p>
												<%= @cwe.terminology_note %>
											</p>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<% unless @cwe.cwe_alternate_terms.blank? %>
		<div id="Alternate_Terms">
			<div class="panel-group" id="alt_term_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapseAltTerm", "data-toggle" => "collapse", "data-parent" => "#alt_term_accordion", "aria-expanded" => "true", "aria-controls" => "collapseDesc" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Alternate Terms
						</div>
					</div>
					<div id="collapseAltTerm" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Alternate_Terms" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Alternate_Terms">
								<div class="detail">
									<div id="Grouped">
										<% @cwe.cwe_alternate_terms.each do |alt_term| %>
											<div class="btmDashed">
												<div class="col-md-3">
													<%= alt_term.term %>
												</div>
												<div class="col-md-9">
													<%= alt_term.desc %>
												</div>
												<div class="clearfix"></div>
											</div>
										<% end %>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<% unless @cwe.cwe_time_of_intros.blank? %>
		<div id="Time_of_Introduction">
			<div class="panel-group" id="alt_term_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapseIntroTime", "data-toggle" => "collapse", "data-parent" => "#intro_time_accordion", "aria-expanded" => "true", "aria-controls" => "collapseIntroTime" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Time of Introduction
						</div>
					</div>
					<div id="collapseIntroTime" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Time_of_Introduction" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Time_of_Introduction">
								<div class="detail">
									<ul>
										<% @cwe.cwe_time_of_intros.each do |time_of_intros| %>
											<% time_of_intros.intro_phase.split(',').each do |time_of_intro| %>
												<li>
													<%= time_of_intro %>
												</li>
											<% end %>
										<% end %>
									</ul>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<div id="Applicable_Platforms">
		<div class="panel-group" id="applicable_platform_accordion" role="tablist" aria-multiselectable="true">
			<div class="panel panel-default">
				<div class="panel-heading" role="tab" id="headingOne">
					<div class="heading">
						<span id="script">
							<%= link_to "#collapseApplicablePlatform", "data-toggle" => "collapse", "data-parent" => "#applicable_platform_accordion", "aria-expanded" => "true", "aria-controls" => "collapseApplicablePlatform" do %>
								<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
							<% end %>
						</span>
						Applicable Platform
					</div>
				</div>
				<div id="collapseApplicablePlatform" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
					<div class="panel-body">
						<div id="oc_<%= @cwe.weakness_id %>_Applicable_Platform" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Applicable_Platform">
							<div class="detail">
								<p class="subheading">Languages</p>
								<div class="indent">
									<p>
										<% @cwe.cwe_languages.each do |language| %>
											<% unless language.language_class.blank? %>
												<%= language.language_class %>
											<% end %>
										<% end %>
									</p>
								</div>
								<% unless @cwe.cwe_architechtural_paradigms.blank? %>
									<p class="subheading">Architechtural Paradigms</p>
									<div class="indent">
										<% @cwe.cwe_architechtural_paradigms.each do |architechtural_paradigm| %>
											<%= architechtural_paradigm.name %>
										<% end %>
									</div>
								<% end %>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<% unless @cwe.cwe_common_consequences.blank? %>
		<div id="Common_Consequences">
			<div class="panel-group" id="common_consequences_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapseCommonConsequences", "data-toggle" => "collapse", "data-parent" => "#common_consequences_accordion", "aria-expanded" => "true", "aria-controls" => "collapseCommonConsequences" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Common Consequences
						</div>
					</div>
					<div id="collapseCommonConsequences" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Common_Consequences" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Common_Consequences">
								<div class="tabledetail">
									<table class="Detail table table-striped" cellspacing="0" cellpadding="0" border="0" width="100%">
										<thead>
											<tr>
												<th>Scope</th>
												<th>Effect</th>
											</tr>
										</thead>
										<tbody>
											<% @cwe.cwe_common_consequences.each do |common_consequence| %>
												<tr>
													<td valign="top">
														<div class="fSize90">
															<%= common_consequence.consequence_scope %>
														</div>
													</td>
													<td valign="top">
														<p class="smaller">
															<span class="subheading">Technical Impact:</span>
															<i>
																<%= common_consequence.technical_impact %>
															</i>
														</p>
														<div class="pTop5">
															<p>
																<%= common_consequence.note %>
															</p>
														</div>
													</td>
												</tr>
											<% end %>
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<% unless @cwe.cwe_demonstrative_examples.blank? %>
		<div id="Demonstrative_Examples">
			<div class="panel-group" id="demonstrative_examples_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapseDemonstrativeExamples", "data-toggle" => "collapse", "data-parent" => "#demonstrative_examples_accordion", "aria-expanded" => "true", "aria-controls" => "collapseDemonstrativeExamples" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Demonstrative Examples
						</div>
					</div>
					<div id="collapseDemonstrativeExamples" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Demonstrative_Examples" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Demonstrative_Examples">
								<div class="detail">
									<% @cwe.cwe_demonstrative_examples.each_with_index do |demo_example, index| %>
										<p class="subheading">
											Example <%= index+1 %>
										</p>
										<p>
											<%= demo_example.intro_text %>
										</p>
										<% demo_example.code_blocks.each do |code_block| %>
											<div id="ExampleCode">
												<div class="CodeHead">
													<div class="optheading col-md-4">
														<span>
															Example Language :
														</span>
														<%= code_block.code_example_lang %>
													</div>
													<div class="col-md-offset-4 block_nature">
														( <%= code_block.block_nature.split("_").join(" ") %> )
													</div>
													<div class="clearfix"></div>
												</div>
											</div>
											<div>
												<pre class="brush: plain;">
													<%= code_block.code_block %>
												</pre>
											</div>
											<p>
												<%= code_block.desc %>
											</p>
										<% end %>
									<% end %>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<% unless @cwe.cwe_observed_examples.blank? %>
		<div id="Observed_Examples">
			<div class="panel-group" id="observed_examples_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapseObservedExamples", "data-toggle" => "collapse", "data-parent" => "#observed_examples_accordion", "aria-expanded" => "true", "aria-controls" => "collapseObservedExamples" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Observed Examples
						</div>
					</div>
					<div id="collapseObservedExamples" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Observed_Examples" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Observed_Examples">
								<div class="tabledetail">
									<table class="Detail table table-striped" cellspacing="0" cellpadding="0" border="0" width="100%">
										<thead>
											<tr>
												<th valign="top" nowrap="">Reference</th>
												<th valign="top" width="100%">Description</th>
											</tr>
										</thead>
										<tbody>
											<% @cwe.cwe_observed_examples.each do |observed_example| %>
												<tr>
													<td valign="top">
														<%= link_to observed_example.observed_example_ref, cfe_path(observed_example.observed_example_ref), target: '_blank' %>
													</td>
													<td valign="top">
														<%= observed_example.desc %>
													</td>
												</tr>
											<% end %>
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<% unless @cwe.liklihood_of_exploit.blank? %>
		<div id="Likelihood_of_Exploit">
			<div class="panel-group" id="likelihood_of_exploit_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapseLikelihoodofExploit", "data-toggle" => "collapse", "data-parent" => "#likelihood_of_exploit_accordion", "aria-expanded" => "true", "aria-controls" => "collapseLikelihoodofExploit" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Likelihood of Exploit
						</div>
					</div>
					<div id="collapseLikelihoodofExploit" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Likelihood_of_Exploit" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Likelihood_of_Exploit">
								<div class="detail">
									<p class="indent">
										<%= @cwe.liklihood_of_exploit %>
									</p>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<% unless @cwe.cwe_detection_methods.blank? %>
		<div id="Detection_Methods">
			<div class="panel-group" id="detection_methods_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapseDetectionMethods", "data-toggle" => "collapse", "data-parent" => "#detection_methods_accordion", "aria-expanded" => "true", "aria-controls" => "collapseDetectionMethods" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Detection Methods
						</div>
					</div>
					<div id="collapseDetectionMethods" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Detection_Methods" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Detection_Methods">
								<div class="detail">
									<div id="Grouped">
										<div class="Detail">
											<% @cwe.cwe_detection_methods.each do |detection_method| %>
												<div valign="top" class="detection_method">
													<p class="subheading">
														<%= detection_method.method_name %>
													</p>
													<div class="indent">
														<%= simple_format(detection_method.method_desc) %>
													</div>
													<p class="optheading">
														<i>
															Effectiveness: <%= detection_method.method_effectiveness %>
														</i>
													</p>
												</div>
											<% end %>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<% unless @cwe.cwe_mitigations.blank? %>
		<div id="Potential_Mitigations">
			<div class="panel-group" id="potential_mitigations_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapsePotentialMitigations", "data-toggle" => "collapse", "data-parent" => "#potential_mitigations_accordion", "aria-expanded" => "true", "aria-controls" => "collapsePotentialMitigations" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Potential Mitigations
						</div>
					</div>
					<div id="collapsePotentialMitigations" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Potential_Mitigations" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Potential_Mitigations">
								<div class="detail">
									<div id="Grouped">
										<div class="Detail">
											<% @cwe.cwe_mitigations.each do |mitigation| %>
												<div valign="top" class="detection_method">
													<p class="subheading">
														Phase: <%= mitigation.mitigation_phase %>
													</p>
													<% unless mitigation.mitigation_strategy.blank? %>
														<p class="optheading">
															Strategy: <%= mitigation.mitigation_strategy %>
														</p>
													<% end %>
													<div>
														<%= mitigation.desc %>
													</div>
												</div>
											<% end %>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<% unless @cwe.cwe_weakness_ordinalities.blank? %>
		<div id="Weakness_Ordinalities">
			<div class="panel-group" id="weakness_ordinalities_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
					<div class="panel-heading" role="tab" id="headingOne">
						<div class="heading">
							<span id="script">
								<%= link_to "#collapseWeaknessOrdinalities", "data-toggle" => "collapse", "data-parent" => "#weakness_ordinalities_accordion", "aria-expanded" => "true", "aria-controls" => "collapseWeaknessOrdinalities" do %>
									<%= image_tag("/assets/head_more.gif", id: "ocimg_200_Description", border: "0", alt: "+") %>
								<% end %>
							</span>
							Weakness Ordinalities
						</div>
					</div>
					<div id="collapseWeaknessOrdinalities" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
						<div class="panel-body">
							<div id="oc_<%= @cwe.weakness_id %>_Weakness_Ordinalities" class="expandblock" name="oc_<%= @cwe.weakness_id %>_Weakness_Ordinalities">
								<div class="tabledetail">
									<table class="Detail table table-striped" cellspacing="0" cellpadding="0" border="0" width="100%">
										<thead>
											<tr>
												<th>Ordinality</th>
												<th>Description</th>
											</tr>
										</thead>
										<tbody>
											<% @cwe.cwe_weakness_ordinalities.each do |weakness_ordinality| %>
												<tr>
													<td valign="top">
														<div class="fSize90">
															<%= weakness_ordinality.ordinality %>
														</div>
													</td>
													<td valign="top">
														<div class="fSize90 ordinality">
															<% if weakness_ordinality.ordinality == "Resultant" %>
																(where the weakness is typically related to the presence of some other weaknesses)
															<% elsif weakness_ordinality.ordinality == "Primary" %>
																(where the weakness exists independent of other weaknesses)
															<% end %>
														</div>
													</td>
												</tr>
											<% end %>
										</tbody>
									</table>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	<% end %>
	<% unless @cwe.cwe_relationships.blank? %>
		<div id="Relationships">
			<div class="panel-group" id="relationships_accordion" role="tablist" aria-multiselectable="true">
				<div class="panel panel-default">
