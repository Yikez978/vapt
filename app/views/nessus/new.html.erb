<%= content_tag :h1 do %>
	Upload Nessus File
<% end %>
<%= form_tag engagement_nessus_path, multipart: true do %>
	<%= content_tag :div, class: 'row' do %>
		<%= content_tag :div, class: 'col-md-4 col-md-offset-4' do %>
			<%= label_tag "Upload nessus file" %>
			<%= file_field_tag "nessus_file" %>
			<%= submit_tag "Submit", class: "nessus_submit" %>
		<% end %>
	<% end %>
<% end %>
