<%= form_tag(engagement_engagement_main_engagement_main_users_path(@engagement, @engagement_main), remote: true) do %>
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">Ã—</span></button>
		<h4 class="modal-title" id="assignUsersModalLabel">Assign Users to <small><%= @engagement_main.host_name %></small></h4>
	</div>
	<div class="modal-body">
		<ul>
			<% @engagement_users.each do |user| %>
			<li>
			  <div class="checkbox">
			    <label for=<%= "user_ids_#{user.id}" %>>
			      <input type="checkbox" name="user_ids[]" id=<%= "user_ids_#{user.id}" %> value=<%= "#{user.id}" %> <%= @existing_user_ids.include?(user.id) ? 'checked="checked"' : '' %>> 
						<%= user.username %>
			    </label>
			  </div>
			</li>
			<% end %>
		</ul>
	</div>
	<div class="modal-footer">
		<%= link_to "Close", "#", class: "btn btn-default", "data-dismiss" => "modal" %>
		<%= submit_tag "Save", class: "btn btn-primary" %>
	</div>
<% end %>